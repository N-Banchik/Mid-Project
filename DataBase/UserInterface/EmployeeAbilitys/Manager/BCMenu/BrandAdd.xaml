<Window x:Class="UserInterface.EmployeeAbilitys.Manager.BCMenu.BrandAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UserInterface.EmployeeAbilitys.Manager.BCMenu"
        mc:Ignorable="d"
        Title="Add Brand" Height="350" Width="300">
    <Grid>
        <Button x:Name="Add" Content="Add" HorizontalAlignment="Center" Click="Add_Click" Margin="0,235,0,0" VerticalAlignment="Top" Height="43" Width="102"/>
        <Label Content="Brand Name" HorizontalAlignment="Left" Margin="65,61,0,0" VerticalAlignment="Top" Height="33" Width="100"/>
        <Label Content="Manufacturing country " HorizontalAlignment="Left" Margin="65,116,0,0" VerticalAlignment="Top" Height="33" Width="87"/>
        <TextBox x:Name="Name" HorizontalAlignment="Center" Margin="0,94,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" Height="30"/>
        <TextBox x:Name="Manufacturingcountry" HorizontalAlignment="Center" Margin="0,149,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" Height="31"/>
    </Grid>
</Window>
